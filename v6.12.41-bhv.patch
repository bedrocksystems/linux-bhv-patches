+				type = GuestConnABI__GuestLog__KernelAccess__AccessType__WRITE;
+			} else {
+				type = GuestConnABI__GuestLog__KernelAccess__AccessType__READ;
+			}
+			security_kaccess((uint64_t)address, type);
+		}
+	}
+
+
