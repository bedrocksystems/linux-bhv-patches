+        }
+
+        writer.data->header.id = data->meta.source_event_id;
+
+        if ((rv = _brs_evt_write_MmapExecFile(&writer, data))) {
+                brs_guestconn_free_msg(data);
+                brs_guestconn_free_msg(buf);
