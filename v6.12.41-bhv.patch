+		HypABI__FileProtection__##T##__arg__T *volatile violation =    \
+			HypABI__FileProtection__##T##__arg__ALLOC();           \
+                                                                               \
+		rv = populate_context(&violation->context, true);              \
+		if (rv) {                                                      \
+			bhv_fail("%s: BHV cannot retrieve event context",      \
