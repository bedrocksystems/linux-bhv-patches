+#define NAMEOFARG(...) \
+	CONCATENATE(NAMEOFARG, BRS_COUNT_ARGS(__VA_ARGS__))(__VA_ARGS__)
+#define TYPEANDNAMEARG(...) \
+	CONCATENATE(TYPEANDNAMEARG, BRS_COUNT_ARGS(__VA_ARGS__))(__VA_ARGS__)
