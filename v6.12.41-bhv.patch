+{
+	int rv;
+
+	inode_lock(d->d_inode);
+#if LINUX_VERSION_CODE >= KERNEL_VERSION(6, 3, 0)
+	rv = __vfs_setxattr_noperm(&nop_mnt_idmap, d, BRS_XATTR_INFO, info,
