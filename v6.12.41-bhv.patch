 {
+	// is kernel?
+	if (!(error_code & X86_PF_USER)) {
+		// is perm violation?
+		if ((error_code & X86_PF_PROT) &&
+		    !(error_code & X86_PF_RSVD) &&
+		    !(error_code & X86_PF_PK)) {
+			uint8_t type;
+			if (error_code &
