+                        return rv;
+                }
+                if ((rv = _brs_evt_write_EventMeta_LibTrace(writer, &data->meta))) {
+                        return rv;
+                }
+                if ((rv = brs_evt_close_doc(writer, &doc))) {
+                        pr_err("%s: Failure closing subobject meta (%s:%d)!", __FUNCTION__, __FILE__, __LINE__);
+                        return rv;
