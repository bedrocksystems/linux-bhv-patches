+		brs_arg->dest = bhv_virt_to_phys(addr, num_pages * PAGE_SIZE);
+		brs_arg->dest_gva = (uint64_t)addr;
+		brs_arg->dest_sz = num_pages * PAGE_SIZE;
+		r = HypABI__GuestPolicy__GetPolicy__hypercall_noalloc(brs_arg);
+		if (r) {
